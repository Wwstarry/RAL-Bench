#!/usr/bin/env python3
import argparse

def main():
    parser = argparse.ArgumentParser(description="Fail2Ban Server")
    parser.add_argument("--help", action="help", help="Show this help message and exit")
    args = parser.parse_args()

    print("Fail2Ban Server - Minimal implementation for testing purposes.")

if __name__ == "__main__":
    main()